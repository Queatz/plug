body{
	//background:url("../image/grid.png") repeat;
	text-align:center;
	font-family:'arial';
	//padding: 0px;
	m//argin: 0px;
	-webkit-touch-callout: none;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
    /*position: absolute;
    top:0;
    bottom:0;
    left:0;
    right:0;
    color:#ffffff;
    overflow: hidden;*/
    height: 100%;
    position:absolute;
    top:0;
    bottom:0;
    right:0;
    left:0;
}

.terms-nav{
	position:absolute;
	z-index:10;
	width:100%;
	height:200px;
	//background: rgba(255,0,0,0.5);
}

.plugContainer{
	position:relative;
	width:100%;
}

.plugCenter{
	position:absolute;
	top: 50%;
    left: 50%;
    margin-right: -50%;
    transform: translate(-50%, -50%);
}

.plugButton{
	color:rgba(0,0,0,0.6);
	display:block;
	margin: 60px auto 60px auto;
	font-family:'montserrat';
	font-weight:bold;
	font-size:45px;
	height:90px;
	width:400px;
	border:none;
	border-radius:25px;
	background:rgba(255,255,255,0.5)
}
.plugtext{
	font-family:'Montserrat';
	font-weight:normal;
}
.plugh1{
	font-family:'arial';
	font-size:70px;
	font-weight:normal;
	padding:10px 0;
	text-shadow: 0px 2px 2px rgba(0, 0, 0, 0.2);
}

.plugh2{
	font-family:'arial';
	font-size:40px;
	font-weight:normal;
	padding:8px 0;
}

.plugh3{
	font-family:'arial';
	font-size:25px;
	font-weight:normal;
	padding: 6px 0;
}

.plughr{
	border-top: 1px solid #7f7f7f;
	border-left: none;
	border-right: none;
	border-bottom: none;
	width: 96%;
	margin: 0 2% 0 2%;
}

hr.divider{
	border: none;
	border-left: none;
	border-right: none;
	border-bottom: none;
	background:transparent;
}

.blueBack{
	background:linear-gradient(to bottom, #ffffff 0%,#4FB6E2 100%);
	color:#000000;
  min-height: 100%;
  height: 100%;
}

#screen{
    /*position: relative;
    top:0;
    bottom:0;
    left:0;
    right:0; */
    position: relative;
    width:1080px;
    height:1920px;
    display:inline-block;
    overflow: hidden;
    background:linear-gradient(to bottom, #ff5fc6 0%,#a542fd 100%) ;
    color:#ffffff;
}

#header{
	width:100%;
	height:200px;
}

.footer{
	position:absolute;
	bottom:0px;
	width:1080px;
	z-index:100;
}
#terms{
	background: transparent;
	position:relative;
	padding:25px;
	text-align:justify;
}
#terms h2{
	font-face:'Montserrat';
	font-size:20px;
	letter-spacing:2px;
}
#terms h2 strong{
	font-size:25px;
}
#terms-wrapper{
	background: transparent;
	position: relative;
	border-radius:20px;
	margin-top:0;
	margin-bottom:0;
	margin-left:auto;
	margin-right:auto;
	border:12px solid #ffffff;
	border-radius:40px;;
	width:80%;
	overflow-y: scroll;
        overflow-x: hiden;
	/* Prevent native touch events on Windows */
	-ms-touch-action: none;
	/* Prevent text resize on orientation change, useful for web-apps */
	-webkit-text-size-adjust: none;
	-moz-text-size-adjust: none;
	-ms-text-size-adjust: none;
	-o-text-size-adjust: none;
	text-size-adjust: none;
}

#scroller {
	position: absolute;
	/* Prevent elements to be highlighted on tap */
	-webkit-tap-highlight-color: rgba(0,0,0,0);
	/* Put the scroller into the HW Compositing layer right from the start */
	-webkit-transform: translateZ(0);
	-moz-transform: translateZ(0);
	-ms-transform: translateZ(0);
	-o-transform: translateZ(0);
	transform: translateZ(0);
}

#terms-check {
    display:none;
}

#terms-check + label {
	margin: 40px auto 40px auto;
	position: relative;
	left: 23px;
    display:block;
    width:300px;
    height:250px;
    background:url("../image/check.png") left top no-repeat;
}

#terms-check:checked + label {
    background:url("../image/check.png") right top no-repeat;
}
.plugHow{
	height:340px;
}
.plugHowIcon{
	width:250px;
	display:inline-block;
	vertical-align:middle;
}
.plugHowText{
	width:550px;
	padding-left:40px;
	text-align:left;
	display:inline-block;
	vertical-align:middle;
}
.plug-errortext{
	color:#fff;
	text-shadow:0px 0px 5px #ffccc4;
}
.grey{
	color:rgba(255,255,255,0.35);
	transform: scale(0.75) translate(-66%, -66%);
}
.plugHowTextLg{
	font-size:52px;
	font-family:'Montserrat';
	font-weight:bold;
}
.plugHowTextMd{
	font-size:25px;
	padding-top:10px;
	font-family:'Montserrat';
}
.plugHowTextSm{
	font-size:20px;
	padding-top:10px;
	font-family:'Montserrat';
}

.conditional{
	display:none;
}
.conditional.active{
	display:block!important;
}

.logoline{
	font-size:40px;
	padding:1em 0;
	letter-spacing: 0.5px;
	display:inline-block;
}
.plug-logo{
	font-family: 'trenchthin';
	color:#ffad1d;
	font-size:100px;
	margin:0 0.25em;
}
.charge-time{
	font-size:70px;
	padding:0 10px;
}
.touch-to-begin{
    #position:absolute;
	#top:0;
	width:100%;
	background-color:rgba(0,0,0,0.7);
	color:#ffffff;
	font-family: 'Montserrat';
        font-size:60px;
}
